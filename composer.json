{
    "name": "access9/dbtabledump",
    "description": "The database table dumper",
    "keywords": ["yaml","dump","export","cli"],
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Glover",
            "email": "brian@access9.net"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/config": "^5.0|^6.0",
        "symfony/console": "^5.0|^6.0",
        "symfony/yaml": "^5.0|^6.0",
        "doctrine/dbal": "^3.5"
    },
    "require-dev": {
        "ext-reflection": "*",
        "ext-pdo_sqlite": "*",
        "phpunit/phpunit": "^9.5",
        "mikey179/vfsstream": "^1.6",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10.x-dev"
        }
    },
    "bin": ["dump"],
    "autoload": {
        "psr-4": {
            "Access9\\DbTableDump\\": "src/DbTableDump/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Access9\\DbTableDump\\Tests\\": "tests/DbTableDump/"
        }
    }
}
