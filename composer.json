{
    "name": "access9/dbtabledump",
    "description": "The database table dumper",
    "keywords": ["yaml","dump","export","cli"],
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Glover",
            "email": "brian@access9.net"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/config": "~4.3",
        "symfony/console": "~4.3",
        "symfony/dependency-injection": "~4.3",
        "symfony/yaml": "~4.3",
        "doctrine/dbal": "~2.8",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "require-dev": {
        "ext-reflection": "*",
        "ext-pdo_sqlite": "*",
        "phpunit/phpunit": "~8",
        "mikey179/vfsstream": "~1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10.x-dev"
        }
    },
    "bin": ["dump"],
    "autoload": {
        "psr-4": {
            "Access9\\DbTableDump\\": "src/DbTableDump/"
        }
    }
}
